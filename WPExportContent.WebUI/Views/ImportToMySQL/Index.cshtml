@model WPExportContent.WebUI.Models.ImportToMySQLDTO

@{
    ViewData["Title"] = "Index";
}

	<h1>ImportToMySQL</h1>

<h4>Informations are NOT stored!!!</h4>

<hr />
<div class="row">
	<div class="col-md-4">
		@using (Html.BeginForm("ConvertJSONToSQL", "ImportToMySQL", FormMethod.Post, new
		{
			enctype = "multipart/form-data"
		}))
		{
			<div asp-validation-summary="ModelOnly" class="text-danger"></div>
			<div class="form-group">
				<label asp-for="ConnectionString" class="control-label"></label>
				<input asp-for="ConnectionString" class="form-control" />
				<span asp-validation-for="ConnectionString" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="contents" class="control-label"></label>
				<input asp-for="contents" class="form-control" type="file" />
				<span asp-validation-for="contents" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Create" class="btn btn-primary" />
			</div>
		}
			</div>
		</div>

<div>
	<h4>Informations are NOT stored!!!</h4>
</div>

@section Scripts {
	@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
